# n8n Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http

# Database
DB_TYPE=sqlite
DB_SQLITE_DATABASE=./n8n-database.sqlite

# Security
N8N_JWT_SECRET=your-jwt-secret-change-this-in-production
N8N_JWT_DURATION_HOURS=24

# Timezone
GENERIC_TIMEZONE=Europe/Warsaw

# Logging
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=console,file

# Development
NODE_ENV=development


